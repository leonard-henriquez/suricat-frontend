{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["jsx-a11y", "import"],
  "rules": {
    "semi": ["error", "never"],
    "react/prefer-stateless-function": 0,
    "import/prefer-default-export": 0
  },
  "env": {
    "es6": true,
    "jest": true,
    "browser": true
  }
}
